{
  "name": "@buildinams/next-lint",
  "description": "BiA's base lint config.",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Build in Amsterdam <development@buildinamsterdam.com> (https://www.buildinamsterdam.com/)",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./next": "./next.js"
  },
  "homepage": "https://github.com/buildinamsterdam/next-lint#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/buildinamsterdam/next-lint.git"
  },
  "bugs": {
    "url": "https://github.com/buildinamsterdam/next-lint/issues"
  },
  "keywords": [
    "react",
    "hooks",
    "hook",
    "react-hook"
  ],
  "scripts": {
    "lint": "NODE_ENV=test npm-run-all --parallel lint:*",
    "lint:script": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:format": "prettier \"**/*.{md,yml}\" --check",
    "lint:type-check": "tsc --noEmit",
    "fix": "npm-run-all --sequential fix:*",
    "fix:js": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "fix:format": "prettier \"**/*.{md,yml}\" --write",
    "depcheck": "npx npm-check --update"
  },
  "peerDependencies": {
    "next": ">=12.0.0",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.2",
    "react-dom": "^18.0.0",
    "typescript": "^4.9.4"
  }
}
